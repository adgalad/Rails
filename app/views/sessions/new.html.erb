
<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '390854754667178',
      xfbml      : true,
      version    : 'v2.10'
    });
    FB.AppEvents.logPageView();
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "https://connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>

<div class="col-sm-3">
</div>
<div class="col-sm-5">
	<div class="center">
	<h1>Login</h1>
		<a href="/auth/google"><%= image_tag("login_google.png", width: "64", height: "64", alt: "") %></a>
		<a href="/auth/facebook"><%= image_tag("login_facebook.png", width: "64", height: "64", alt: "") %></a>
	</div>
		
</div>